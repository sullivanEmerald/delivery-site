    <!-- ======= About Us Section ======= -->
    <section id="create" class="about">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>CREATE A PARCEL</h2>
        </div>

        
          <div id="content">
            
            <form action="/createparcel" enctype="multipart/form-data" method="POST">
                <h4>Sender's information</h4>
                <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter sender name" name="sendername">
                  </div>
        
                  <div class="form-floating">
                    <input type="phone" class="form-control" id="floatingInput" placeholder="enter sender phone number" name="sendernumber">
                  </div>
        
                  <div class="form-floating">
                    <input type="email" class="form-control" id="floatingInput" placeholder="enter sender email" name="senderemail">
                  </div>
        
                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter sender address" name="senderaddress">
                  </div>

                  <h4>Reciever's information</h4>

                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter reciever name" name="recievername">
                  </div>
        
                  <div class="form-floating">
                    <input type="phone" class="form-control" id="floatingInput" placeholder="enter reciever phone number" name="recievernumber">
                  </div>
        
                  <div class="form-floating">
                    <input type="email" class="form-control" id="floatingInput" placeholder="enter reciever email" name="recieveremail">
                  </div>
        
                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter reciever address" name="recieveraddress">
                  </div>

                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter reciever landmark" name="recieverlandmark">
                  </div>

                  <h4>Parcel information</h4>
        
                  <div class="mb-3">
                    <label for="imgUpload" class="form-label">Image</label>
                    <input type="file" class="form-control" id="imageUpload" name="file">
                  </div>


                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter departing station or country" name="parcelstation">
                  </div>

                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter parcel status(send,pending, delivered, transit, custom withheld)" name="parcelstatus">
                  </div>

                  <div class="form-floating">
                    <input type="text" class="form-control" id="floatingInput" placeholder="enter state of delivery" name="arrival">
                  </div>

                  <div class="form-floating">
                    <input type="date" class="form-control" id="floatingInput" placeholder="Date OF Arrival" name="date">
                  </div>
                  
    
                <button type="submit" class="btn btn-outline-primary btn-lg">Create Parcel</button>
              </form>
