<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title><%= title %></title>
  <meta content="" name="description">
  <meta content="" name="keywords">

 <!-- link to the favicon -->
<link rel="icon" type="image/png" sizes="32x32" href="/images/icons/favicon.png"/>

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Jost:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/assets/vendor/icofont/icofont.min.css" rel="stylesheet">
  <link href="/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="/assets/vendor/venobox/venobox.css" rel="stylesheet">
  <link href="/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="/assets/vendor/aos/aos.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/css/style.css" rel="stylesheet">


</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top ">
    <div class="container d-flex align-items-center">

      <h1 class="logo mr-auto"><a href="/">Euro Transit</a></h1>
      <!-- Uncomment below if you prefer to use an image logo -->
      <!-- <a href="index.html" class="logo mr-auto"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->

      <nav class="nav-menu d-none d-lg-block">
        <ul>
          <li class="active"><a href="/">Home</a></li>

        </ul>
      </nav><!-- .nav-menu -->


    </div>
  </header><!-- End Header -->

  <!-- ======= Hero Section ======= --> 
  <section id="hero" class="d-flex align-items-center">

    <div class="container">
      <div class="row">
        <div class="col-lg-6 d-flex flex-column justify-content-center pt-4 pt-lg-0 order-2 order-lg-1" data-aos="fade-up" data-aos-delay="200">
          <h1>Euro Transit</h1>
          <h2>TRACKING DELIVERY COMPANY</h2>
          <p style="color: #fff;">Your Lightning Fast Delivery, provides international and local courier service to customers around the world</p>
          <div class="" style="width: 100%;">
            <form action="/findparcel" method="POST">
              <div class="mb-3 trackInput" style="position: relative;" >
                <input type="text" class="form-control" placeholder="Tracking ID" style="height: 60px; border-radius: 10px;" name="parcel">
                <button class="btn btn-primary btn-lg" id="heroTrackButton" type="submit">Find Order</button>
              </div>
            </form>
          </div>
        </div>
        <div class="col-lg-6 order-1 order-lg-2 hero-img" data-aos="zoom-in" data-aos-delay="200">
          <img src="/images/heroplane2.png" class="img-fluid animated" alt="">
        </div>
      </div>
    </div>

  </section><!-- End Hero -->

  <main id="main">


    <!-- ======= About Us Section ======= -->
    <section id="create" class="">
      <div class="container" data-aos="fade-up">
        <div class="section-title">
          <h2>Parcel Informations</h2>
          <h4>Tracking code : <%= user.parcelcode %> </h4>
          <h4>Date of Arrival : <%= user.eta %> </h4>
        </div>
        <div class="parcelPage">
            <section>
                <section class="firstsection">
                    <h4 class="text-info">Parcel Details</h4>
                    <p>TIme of Order : <%= user.time %> </p>
                    <p>Date of Order : <%= user.date %> </p> 
                    <p>ETA : <%= user.eta %> </p>
                    <p class="text-danger" style="font-weight: bold; font-size : 20px;">Status : <%= user.parcelstatus %> </p> 
                    <p>Receipt N0 : <%= user.parcelreference %> </p> 
                    <p>Track ID : <%= user.parcelcode %> </p>
                    <img src="<%= user.parcelimage %>" alt=""> 
                </section>
                <section>
                    <h4 class="text-info">Sender Informations</h4>
                    <p>Name : <%= user.sendername %> </p>
                    <p>Email : <%= user.senderemail %> </p>
                    <p>Phone Number : <%= user.sendernumber %> </p>
                    <p>Address : <%= user.senderaddress %> </p>
                </section>
                <section>
                    <h4 class="text-info">Reciever Informations</h4>
                    <p>Name : <%= user.recievername %> </p>
                    <p>Email : <%= user.recieveremail %> </p>
                    <p>Phone Number : <%= user.recievernumber %> </p>
                    <p>Address : <%= user.recieveraddress %> </p>
                    <p>Land mark : <%= user.recieverlandmark %> </p>

                </section>
            </section>
          </div>
      </div>
    </section><!-- End About Us Section -->
 


  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <footer id="footer">

    <div class="footer-top">
      <div class="container">
        <div class="row">

          <div class="col-lg-3 col-md-6 footer-contact">
            <h3>Euro Transit</h3>
            <p>
              A108 Adam Street <br>
              New York, NY 535022<br>
              United States <br><br>
              <strong>Phone:</strong> +1 5589 55488 55<br>
              <strong>Email:</strong> info@example.com<br>
            </p>
          </div>

          <div class="col-lg-3 col-md-6 footer-links">
            <h4>Useful Links</h4>
            <ul>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Home</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">About us</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Services</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Terms of service</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Privacy policy</a></li>
            </ul>
          </div>

          <div class="col-lg-3 col-md-6 footer-links">
            <h4>Our Services</h4>
            <ul>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Storage</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Logistics</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Cargo</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Trucking</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">Packaging</a></li>
              <li><i class="bx bx-chevron-right"></i> <a href="#">warehousing</a></li>
            </ul>
          </div>

          <div class="col-lg-3 col-md-6 footer-links">
            <h4>Our Social Networks</h4>
            <p>Connect with on our various social media handles</p>
            <div class="social-links mt-3">
              <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
              <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
              <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
              <a href="#" class="google-plus"><i class="bx bxl-skype"></i></a>
              <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
            </div>
          </div>

        </div>
      </div>
    </div>

    <div class="container footer-bottom clearfix">
      <div class="copyright">
        &copy; Copyright <strong><span>Euro Transit</span></strong>. All Rights Reserved
      </div>
    </div>
  </footer><!-- End Footer -->

  <a href="#" class="back-to-top"><i class="ri-arrow-up-line"></i></a>
  <div id="preloader"></div>
  
 
  <!-- Vendor JS Files -->
  <script src="/assets/vendor/jquery/jquery.min.js"></script>
  <script src="/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
  <script src="/assets/vendor/php-email-form/validate.js"></script>
  <script src="/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
  <script src="/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="/assets/vendor/venobox/venobox.min.js"></script>
  <script src="/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
  <script src="/assets/vendor/aos/aos.js"></script>

  <!-- Template Main JS File -->
  <script src="/js/main.js"></script>

</body>

</html>